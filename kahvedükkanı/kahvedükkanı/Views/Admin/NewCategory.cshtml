@model kahvedükkanı.Models.Siniflar.AnaSayfa
@{
    ViewBag.Title = "NewCategory";
    Layout = "~/Views/Shared/_AdminLayoyt.cshtml";
}

<h2>Add New Category</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.CategoryName)
    @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" }) @*css ekleme*@
    <br />

    @Html.LabelFor(x => x.CategoryImgUrl)
    @Html.TextBoxFor(x => x.CategoryImgUrl, new { @class = "form-control" }) @*css ekleme*@
    <br />
    <button class="btn btn-success">Add Category</button>
</form>

